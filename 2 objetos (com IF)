class Produto {
  String nome;
  double preco;
  int estoque;
}

public class Main {
  public static void main(String[] args) {
    Produto produto = new Produto();
    Produto produto2 = new Produto();
    
    produto.nome = "Shampoo";
    produto.preco = 25.99;
    produto.estoque = 2;
    
    System.out.println("Produto: " + produto.nome);
    System.out.println("Preço: " + produto.preco);
    System.out.println("Total em estoque: " + produto.estoque * produto.preco);
    
    produto2.nome = "Condicionador";
    produto2.preco = 22.99;
    produto2.estoque = 2;
    
    System.out.println("\nProduto: " + produto2.nome);
    System.out.println("Preço: " + produto2.preco);
    System.out.println("Total em estoque: " + produto2.estoque * produto2.preco);
    
    if(produto.preco > produto2.preco){
      System.out.println("\nProduto mais caro: " + produto.nome + " -- R$ " + produto.preco);
    } else{
        System.out.println("\nProduto mais caro: "+ produto2.nome + " -- R$ " + produto.preco);
    
      }
  }
}
